{
  "name": "spec-schema-org",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "22.22.0"
  },
  "scripts": {
    "clean": "node --eval \"import { rmSync } from 'node:fs'; rmSync('_site', { recursive: true, force: true });\"",
    "_11ty": "eleventy --config=eleventy.config.ts",
    "dev": "npm run _11ty -- --serve",
    "build": "npm run clean && npm run _11ty",
    "format": "node scripts/format.mjs",
    "format:check": "npm run format -- --check"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "prettier": "^3.0.0"
  }
}
